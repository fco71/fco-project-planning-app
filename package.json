{
  "name": "fco-planning-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "setup:dev": "npm install && npm run hooks:install && npm run hooks:verify",
    "build": "vite build",
    "build:offline": "VITE_OFFLINE_MODE=1 vite build",
    "test": "vitest run",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "check:changed": "./scripts/dev/check-changed.sh",
    "hooks:install": "./scripts/dev/install-git-hooks.sh",
    "hooks:status": "git config --get core.hooksPath || true",
    "hooks:verify": "./scripts/dev/verify-hooks.sh",
    "guard:secrets": "./scripts/ci/check-tracked-secrets.sh",
    "guard:secrets:staged": "./scripts/ci/check-tracked-secrets.sh --scope=staged",
    "guard:secrets:changed": "./scripts/ci/check-tracked-secrets.sh --scope=changed --base-ref=main",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "check": "npm run lint && npm run typecheck",
    "preview": "vite preview"
  },
  "dependencies": {
    "firebase": "^12.8.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.58.2",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "firebase-tools": "^15.6.0",
    "globals": "^16.5.0",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
